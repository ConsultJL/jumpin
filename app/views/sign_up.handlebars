<body class="login breakpoint-480">
<div id="content">
  {{> navbar }}
    <br />
    <div class="container-fluid">
        <div class="lock-container">
            <h1>Account Access</h1>
            <div class="panel panel-default text-center">
                <div class="panel-body">
                    {{#if message}}
                      <div class="alert alert-danger">{{message}}</div>
                    {{/if}}
                    <p>Welcome to JumpIn! Please fill out the form below to signup for our awesome service!</p>
                    <form action="/sign_up" method="post">
                      <input class="form-control" type="test" name="firstName" placeholder="First Name" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <input class="form-control" type="test" name="lastName" placeholder="Last Name" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <input class="form-control" type="test" name="profilePic" placeholder="Profile Pic URL" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <input class="form-control" type="test" name="email" placeholder="Enter Email" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <input class="form-control" type="text" name="username" placeholder="Username" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <input class="form-control" type="password" name="password" placeholder="Enter Password" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <input class="form-control" type="password" name="confirm_password" placeholder="Enter Confirm Password" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat;">
                      <div class="radio radio-primary">
                        <input type="radio" id="studentRole" name="role" value="1" checked/>
                        <label for="studentRole">Student</label>
                      </div>
                      <div class="radio radio-primary">
                        <input type="radio" id="teacherRole" name="role" value="2"/>
                        <label for="teacherRole">Teacher</label>
                      </div>
                      <button type="submit" class="btn btn-primary">Sign Up</button>
                    </form>
                </div>
            </div>
        </div>

    </div>
</div>
